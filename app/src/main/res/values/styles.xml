<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="listDetailsTitle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="listDetailsData">
        <item name="android:textSize">13sp</item>
    </style>

    <style name="Aocpv.HeaderTitle" parent="">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="Aocpv.ValidationTitle" parent="Aocpv.HeaderTitle">
        <item name="android:background">@color/primaryColor</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">4dp</item>
    </style>

    <style name="Aocpv.SubTitle" parent="Aocpv.HeaderTitle">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Aocpv.Data" parent="">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="FilterTitle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="FilterData">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>